import java.util.HashSet;
import java.util.Set;

public class RutThamTrungThuong_HashSet {
	public static void main(String[] args) {
		Set<String> thungPhieuRutThamTrungThuong = new HashSet<String>();
		
		int luaChon = 0;
		do {
			for (int i = 0; i < 50; i++) {
				System.out.print("-");
			}
			System.out.println("\nMenu: ");
			System.out.println("\n1.	Thêm mã số dự thưởng.\r\n"
					+ "2.	Xóa mã số dự thưởng\r\n"
					+ "3.	Kiểm tra mã số dự thưởng có tồn tại hay không?\r\n"
					+ "4.	Xóa tất cả mã số dự thưởng.\r\n"
					+ "5.	Số lượng phiếu dự thưởng.\r\n"
					+ "6.	Rút thăm trúng thưởng.\r\n"
					+ "0.	Thoát khỏi chương trình.\r\n"
					+ "");
		} while (luaChon !=0);
	}
}
